{
    "name": "hisman/laravel-make-class",
    "description": "Artisan command for generating a new custom class in Laravel.",
    "license": "MIT",
    "authors": [
        {
            "name": "Hisman",
            "homepage": "https://hisman.co",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Hisman\\MakeClass\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hisman\\MakeClass\\Tests\\": "tests/"
        }
    },
    "require": {},
    "require-dev": {
        "phpunit/phpunit": "^7.4",
        "orchestra/testbench": "^3.7",
        "mockery/mockery": "^1.2"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Hisman\\MakeClass\\MakeClassServiceProvider"
            ]
        }
    }
}
